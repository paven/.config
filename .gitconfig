[user]
	email = pvn@textalk.se
	name = Patrik Gustafsson
[core]
	whitespace = trailing-space,space-before-tab
	quotepath = off
	excludesfile = /home/pvn/.config/git/ignore
	pager = less -FRXK
[merge]
	ff = only
	tool = vimdiff
[apply]
	whitespace = fix
[branch "master"]
	mergeoptions = --ff-only
[push]
	default = simple
[branch]
	autosetuprebase = always
[alias]
	b = !birgit
	bir = !birgit
	birgit = !birgit
[diff "xlsx"]
	binary = true
	textconv = /usr/bin/unoconv -f csv --stdout
[pull]
	rebase = true
[fetch]
	prune = true
[diff]
	guitool = kdiff3
[difftool "kdiff3"]
	path = /usr/bin/which: nogitf kdiff3 in (/home/pvn/bin:/usr/lib64/qt-3.3/bin:/usr/local/bin:/usr/local/sbin:/usr/bin:/usr/sbin:/sbin:/home/pvn/bin:/home/pvn/opt/netbeans-8.0.1/bin/:/home/pvn/.local/bin:/home/pvn/bin:/home/pvn/bin:/home/pvn/opt/netbeans-8.0.1/bin/:/usr/lib64/qt-3.3/bin:/usr/local/bin:/usr/local/sbin:/usr/bin:/usr/sbin:/home/pvn/bin)
[status]
	submoduleSummary = true
[rebase]
	missingCommitsChec = warn
[color]
	ui = auto
